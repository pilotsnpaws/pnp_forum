{
    "name": "sitesplat/cloudflareip",
    "type": "phpbb-extension",
    "description": "This extension normalizes user IP addresses while your server is under Cloudflare proxy.",
    "homepage": "http://sitesplat.com",
    "version": "1.0.0",
    "keywords": [
        "phpbb",
        "extension",
        "ip",
        "UCP",
        "session",
        "security",
        "sitesplat"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "ThemeSplat (SiteSplat)",
            "email": "info@sitesplat.com",
            "homepage": "http://www.sitesplat.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "composer/installers": "~1.0",
        "phpbb/phpbb": ">=3.1.10,<3.3.0@dev"
    },
    "extra": {
        "display-name": "CloudFlare IP",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.10,<3.3.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/cloudflare_ip",
            "filename": "version_check"
        }
    }
}